{
  "name": "customappgate",
  "displayName": "customappgate",
  "description": "",
  "version": "0.16.0",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "customappgate.entitlementscript",
        "title": "entitlement script"
      },
      {
        "command": "customappgate.resetcreds",
        "title": "Change/Reset Collective/Creds"
      },
      {
        "command": "customappgate.criteriascript",
        "title": "condition/criteria script"
      },
      {
        "command": "customappgate.collective",
        "title": "Current Collective"
      },
      {
        "command": "customappgate.configure",
        "title": "Configure AG Script"
      },
      {
        "command": "customappgate.userclaimsscript",
        "title": "user claims script"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "customappgate.criteriascript",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        },
        {
          "command": "customappgate.userclaimsscript",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        },
        {
          "command": "customappgate.entitlementscript",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        }
      ]
    }
  },
  "scripts": {
    "test-compile": "tsc -p ./",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.67.0",
    "@vscode/test-electron": "^2.1.3",
    "esbuild": "^0.14.39",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "uuid": "^8.3.2"
  }
}
